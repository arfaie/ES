
<!DOCTYPE html>
<html lang="en" dir="rtl">

<!-- Mirrored from demo.naksoid.com/elephant-v1.3.0/theme-1-rtl/blank-page.html by HTTrack Website Copier/3.x [XR&CO'2010], Wed, 15 Nov 2017 15:03:12 GMT -->
<head>

    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>@ViewBag.Title</title>
    <link rel="icon" type="~/Content/header-logo.png" href="~/Content/header-logo.png" sizes="32x32">
    <link rel="icon" type="~/Content/header-logo.png" href="~/Content/header-logo.png" sizes="16x16">
    <link rel="manifest" href="manifest.json">
    <link rel="mask-icon" href="safari-pinned-tab.svg" color="#d9230f">
    <meta name="theme-color" content="#ffffff">
    <link href="~/Content/AdminHtml/css/elephant-rtl.min.css" rel="stylesheet" />
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link href="~/Scripts/TimePicker/wickedpicker.min.css" rel="stylesheet" />
    <script src="~/Scripts/PunasMarketing.js"></script>
    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
    <link href="~/Content/Font/Sahel.css" rel="stylesheet" />
    @Styles.Render("~/Content/Picker")
    @Styles.Render("~/Content/themes/base/css")
    @RenderSection("styles", false)

</head>
<body class="layout layout-header-fixed" style="font-family: 'Sahel';font-size:medium">

    <div class="layout-header">
        <div class="navbar navbar-default">
            <div class="navbar-header">

                <a class="navbar-brand navbar-brand-center" href=#>
                    <div>پنل مدیریت</div>
                </a>
                <button class="navbar-toggler visible-xs-block collapsed" type="button" data-toggle="collapse" data-target="#sidenav">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="bars">
                        <span class="bar-line bar-line-1 out"></span>
                        <span class="bar-line bar-line-2 out"></span>
                        <span class="bar-line bar-line-3 out"></span>
                    </span>
                    <span class="bars bars-x">
                        <span class="bar-line bar-line-4"></span>
                        <span class="bar-line bar-line-5"></span>
                    </span>
                </button>
                <button class="navbar-toggler visible-xs-block collapsed" type="button" data-toggle="collapse" data-target="#navbar">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="arrow-up"></span>
                    <span class="ellipsis ellipsis-vertical">
                        <img class="ellipsis-object" width="32" height="32" src="~/Content/header-logo.png" alt="Menue">
                    </span>
                </button>
            </div>
            <div class="navbar-toggleable">
                <nav id="navbar" class="navbar-collapse collapse">
                    <span class="h4" style="line-height: 51px;text-align :center;padding-right:10px;font-family :'Sahel' ; ">سیستم مدیریت آژانس خسروشاه</span>
                    <button class="sidenav-toggler hidden-xs" title="Collapse sidenav ( [ )" aria-expanded="true" type="button">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="bars">
                            <span class="bar-line bar-line-1 out"></span>
                            <span class="bar-line bar-line-2 out"></span>
                            <span class="bar-line bar-line-3 out"></span>
                            <span class="bar-line bar-line-4 in"></span>
                            <span class="bar-line bar-line-5 in"></span>
                            <span class="bar-line bar-line-6 in"></span>
                        </span>
                    </button>
                    @*@Html.Action("NavBar", "Admin")*@
                </nav>
            </div>
        </div>
    </div>
    <div class="layout-main">
        <div class="layout-sidebar">
            <div class="layout-sidebar-backdrop"></div>
            <div class="layout-sidebar-body">
                <div class="custom-scrollbar">
                    <nav id="sidenav" class="sidenav-collapse collapse">

                            <ul class="sidenav">
                                <li class="sidenav-item">
                                    <a href="/Agency/" aria-haspopup="true">
                                        <span class="sidenav-icon icon icon-cab"></span>
                                        <span class="sidenav-label">مدیریت آژانس ها</span>
                                    </a>
                                </li>
                                <li class="sidenav-item">
                                    <a href="/Driver/" aria-haspopup="true">
                                        <span class="sidenav-icon icon icon-male"></span>
                                        <span class="sidenav-label">مدیریت رانندگان</span>
                                    </a>
                                </li>
                                <li class="sidenav-item">
                                    <a href="/Travel/" aria-haspopup="true">
                                        <span class="sidenav-icon icon icon-tasks"></span>
                                        <span class="sidenav-label">سفر های ثبت شده</span>
                                    </a>
                                </li>
                                <li class="sidenav-item">
                                    <a href="/Report/Report/" aria-haspopup="true">
                                        <span class="sidenav-icon icon icon-cubes"></span>
                                        <span class="sidenav-label">گزارش گیری</span>
                                    </a>
                                </li>
                                <li class="sidenav-item">
                                    <a href="/Admin/Index/" aria-haspopup="true">
                                        <span class="sidenav-icon icon icon-users"></span>
                                        <span class="sidenav-label">مدیریت کاربران</span>
                                    </a>
                                </li>
                            </ul>
                        
                        @*else { Response.Redirect("/Admin/Login"); }*@

                            <ul class="sidenav">
                                <li class="sidenav-item">
                                    <a href="/Travel/" aria-haspopup="true">
                                        <span class="sidenav-icon icon icon-tasks"></span>
                                        <span class="sidenav-label">سفر های ثبت شده</span>
                                    </a>
                                </li>
                            </ul>

                    </nav>
                </div>
            </div>
        </div>
        <div class="layout-content">
            @RenderBody()
        </div>
        <div class="layout-footer">
            <div class="layout-footer-body">
                <small class="version" style="font-family:'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;font-size:larger">Agency CMS</small>
            </div>
        </div>
    </div>




    <script src="~/Scripts/jquery-3.2.1.min.js"></script>

    <script src="~/Scripts/TimePicker/wickedpicker.js"></script>
    <script type="text/javascript">
        $('.timepicker-24-hr').wickedpicker({ twentyFour: true, title: "انتخاب زمان" });
    </script>


    @RenderSection("toastr", false)



    <script src="~/Content/AdminHtml/js/vendor.min.js"></script>
    <script src="~/Content/AdminHtml/js/elephant.min.js"></script>
    <script src="~/Content/AdminHtml/js/application.min.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>

    <script src="~/Content/AdminHtml/js/demo.min.js"></script>
    <script src="~/Scripts/Utility.js"></script>

    @RenderSection("scripts", required: false)
</body>
</html>